{
  "name": "@sickvibes/vibes-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "webpack serve --config=./webpack.dev.js",
    "build": "webpack build --config=./webpack.build.js",
    "clean": "rm -rf ./dist ./.cache"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "files": [
      "./src/**/*.ts"
    ],
    "printWidth": 120,
    "parser": "typescript"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:prettier/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "**/.serverless/*",
      "**/node_modules/*",
      "**/generated-*",
      "**/*snapshot.spec.ts"
    ],
    "rules": {}
  },
  "devDependencies": {
    "@types/jest": "26.0.18",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "css-loader": "5.2.6",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "html-webpack-plugin": "5.3.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hot-loader": "4.13.0",
    "react-router-dom": "5.2.0",
    "style-loader": "2.0.0",
    "ts-jest": "26.5.0",
    "ts-loader": "8.0.18",
    "typescript": "4.1.3",
    "webpack": "5.27.1",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "@material-ui/styles": "4.11.3",
    "@types/lodash": "4.14.171",
    "@web3-react/core": "6.1.1",
    "@web3-react/injected-connector": "6.0.7",
    "ethers": "5.0.29",
    "ethers-multicall": "0.2.1",
    "lodash": "4.17.21",
    "react-markdown": "6.0.2"
  }
}
